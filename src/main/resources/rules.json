{
  "rules": [
    {
      "methods": ["GET"],
      "pattern": "/konferencije/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["POST","PATCH","DELETE"],
      "pattern": "/konferencije/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/ocjene/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/posjetioci/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET","DELETE", "POST", "PATCH"],
      "pattern": "/rezervacije/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/resursi/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/lokacije/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/tipoviDOgadjaja/*",
      "roles": ["ORGANIZATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/konferencije/*/moderator-conf",
      "roles": [
        "MODERATOR"
      ]
    },
    {
      "methods": ["POST","PATCH"],
      "pattern": "/ocjene",
      "roles": ["MODERATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/ocjene/*",
      "roles": ["MODERATOR"]
    },
    {
      "methods": ["PATCH"],
      "pattern": "/korisnici/*/status",
      "roles": [
        "ADMIN"
      ]
    },
    {
      "methods": ["PATCH"],
      "pattern": "/korisnici/*/role",
      "roles": [
        "ADMIN"
      ]
    },
    {
      "methods": ["GET"],
      "pattern": "/korisnici/*",
      "roles": [
        "ADMIN"
      ]
    },
    {
      "methods": ["POST"],
      "pattern": "/posjetioci",
      "roles": ["POSJETILAC"]
    },
    {
      "methods": ["GET"],
      "pattern": "/konferencije/*",
      "roles": ["POSJETILAC"]
    },
    {
      "methods": ["GET"],
      "pattern": "/konferencije/status/*",
      "roles": ["POSJETILAC","ORGANIZATOR","MODERATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/konferencije/datum/*",
      "roles": ["POSJETILAC","ORGANIZATOR","MODERATOR"]
    },
    {
      "methods": ["GET"],
      "pattern": "/konferencije/naziv/*",
      "roles": ["POSJETILAC","ORGANIZATOR","MODERATOR"]
    }
  ]
}